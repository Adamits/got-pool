<!DOCTYPE html>
<html>
<head>
  <title>GotPool</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <div class="top">
    <div class="container">
      <%= render "shared/nav" %>
    </div>
    <%= render "shared/characters" %>
  </div>
  <div class="container leaderboard">
  <%= yield %>
</body>
</html>
